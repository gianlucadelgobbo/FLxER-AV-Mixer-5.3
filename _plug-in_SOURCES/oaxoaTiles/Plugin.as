package {	public class Plugin {		public static var pref;		var c;		var loader;		public function Plugin() {		}		public static function getChannelsNum() {			return Preferences.pref.nCh;		}		public static function getMonWidth() {			return 800;		}		public static function getMonHeight() {			return 600;		}		public static function getChannel(ch) {			return Preferences.pref.monitorTrgt.levels["ch_"+ch];		}		public static function getMovie(ch) {			return Preferences.pref.monitorTrgt.levels["ch_"+ch].vid;		}		public static function getMovieCnt(ch) {			return Preferences.pref.monitorTrgt.levels["ch_"+ch].cnt;		}		public static function getMask(ch) {			return Preferences.pref.monitorTrgt.levels["ch_"+ch].cntMask;		}		public static function clearMask(ch){			Preferences.pref.monitorTrgt.levels["ch_"+ch].clearMask()		}		public static function restoreMask(ch){			Preferences.pref.monitorTrgt.levels["ch_"+ch].restoreMask()		}	}}